local SPECIAL_POS = Vector3.new(-21.042, 153.328, 503.786)

local RUNNING = false           
local PLAYER_COUNTER = 0
local MINIMIZED = false


local gui_toggle_button = nil
local gui_name_label = nil
local gui_coord_label = nil
local gui_avatar = nil


local function safe(fn, ...)
    local ok, res = pcall(fn, ...)
    if not ok then return nil end
    return res
end

local function getThumbByName(name)
    local id = safe(function() return Players:GetUserIdFromNameAsync(name) end)
    if not id then return nil end
    return safe(function() return Players:GetUserThumbnailAsync(id, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100) end)
end

local DEFAULT_AVATAR = safe(function() return getThumbByName(S.catto_name) end) or "rbxasset://textures/ui/GuiImagePlaceholder.png"

local function buildGui()
    local gui = Instance.new("ScreenGui")
    gui.Name = "TPGui_" .. tostring_t(math_random(1000,9999))
    gui.ResetOnSpawn = false
    gui.Parent = lp:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 340, 0, 200)
    frame.Position = UDim2.new(0.35, 0, 0.65, 0)
    frame.BackgroundColor3 = Color3.fromRGB(28, 28, 28)
    frame.BorderSizePixel = 0
    frame.Active = true
    frame.Draggable = true
    frame.Parent = gui
    Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 12)

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -30, 0, 28)
    title.Position = UDim2.new(0, 0, 0, 0)
    title.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
    title.BorderSizePixel = 0
    title.Text = S.title
    title.TextColor3 = Color3.fromRGB(235, 235, 235)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 16
    title.TextXAlignment = Enum.TextXAlignment.Center
    title.Parent = frame

    local minimizeBtn = Instance.new("TextButton")
    minimizeBtn.Size = UDim2.new(0, 28, 0, 28)
    minimizeBtn.Position = UDim2.new(1, -28, 0, 0)
    minimizeBtn.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    minimizeBtn.Text = "–"
    minimizeBtn.TextColor3 = Color3.new(1, 1, 1)
    minimizeBtn.Font = Enum.Font.GothamBold
    minimizeBtn.TextSize = 18
    minimizeBtn.Parent = frame
    Instance.new("UICorner", minimizeBtn).CornerRadius = UDim.new(0, 6)

    local avatar = Instance.new("ImageLabel")
    avatar.Size = UDim2.new(0, 50, 0, 50)
    avatar.Position = UDim2.new(0, 10, 0, 40)
    avatar.BackgroundTransparency = 1
    avatar.Image = DEFAULT_AVATAR
    avatar.Parent = frame

    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, -80, 0, 22)
    nameLabel.Position = UDim2.new(0, 70, 0, 42)
    nameLabel.BackgroundTransparency = 1
    nameLabel.TextColor3 = Color3.new(1, 1, 1)
    nameLabel.Text = "Target: None"
    nameLabel.Font = Enum.Font.Gotham
    nameLabel.TextSize = 15
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Parent = frame

    local coordLabel = Instance.new("TextLabel")
    coordLabel.Size = UDim2.new(1, -80, 0, 20)
    coordLabel.Position = UDim2.new(0, 70, 0, 65)
    coordLabel.BackgroundTransparency = 1
    coordLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    coordLabel.Text = "Coords: N/A"
    coordLabel.Font = Enum.Font.Gotham
    coordLabel.TextSize = 13
    coordLabel.TextXAlignment = Enum.TextXAlignment.Left
    coordLabel.Parent = frame

    local buttonRow = Instance.new("Frame")
    buttonRow.Size = UDim2.new(1, -20, 0, 40)
    buttonRow.Position = UDim2.new(0, 10, 1, -60)
    buttonRow.BackgroundTransparency = 1
    buttonRow.Parent = frame

    local toggleBtn = Instance.new("TextButton")
    toggleBtn.Size = UDim2.new(0.5, -5, 1, 0)
    toggleBtn.Position = UDim2.new(0, 0, 0, 0)
    toggleBtn.BackgroundColor3 = Color3.fromRGB(60, 170, 250)
    toggleBtn.TextColor3 = Color3.new(1, 1, 1)
    toggleBtn.Text = S.enable
    toggleBtn.Font = Enum.Font.GothamBold
    toggleBtn.TextSize = 16
    toggleBtn.Parent = buttonRow
    Instance.new("UICorner", toggleBtn).CornerRadius = UDim.new(0, 8)

    local discordBtn = Instance.new("TextButton")
    discordBtn.Size = UDim2.new(0.5, -5, 1, 0)
    discordBtn.Position = UDim2.new(0.5, 5, 0, 0)
    discordBtn.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
    discordBtn.TextColor3 = Color3.new(1, 1, 1)
    discordBtn.Text = "Copy Discord"
    discordBtn.Font = Enum.Font.GothamBold
    discordBtn.TextSize = 16
    discordBtn.Parent = buttonRow
    Instance.new("UICorner", discordBtn).CornerRadius = UDim.new(0, 8)

    local credits = Instance.new("TextLabel")
    credits.Size = UDim2.new(1, -20, 0, 15)
    credits.Position = UDim2.new(0, 10, 1, -20)
    credits.BackgroundTransparency = 1
    credits.TextColor3 = Color3.fromRGB(150, 150, 150)
    credits.Text = S.madeby
    credits.Font = Enum.Font.Gotham
    credits.TextSize = 12
    credits.TextXAlignment = Enum.TextXAlignment.Center
    credits.Parent = frame

    minimizeBtn.MouseButton1Click:Connect(function()
        MINIMIZED = not MINIMIZED
        for _, child in ipairs(frame:GetChildren()) do
            if child ~= title and child ~= minimizeBtn then
                child.Visible = not MINIMIZED
            end
        end
        frame.Size = MINIMIZED and UDim2.new(0, 340, 0, 28) or UDim2.new(0, 340, 0, 200)
    end)

    discordBtn.MouseButton1Click:Connect(function()
        if setclipboard then
            pcall(function()
                setclipboard(S.discord)
                -- silent: do not call SetCore so nothing prints/appears in Studio
            end)
        else
            coordLabel.Text = "Discord: " .. S.discord_short
        end
    end)

    gui_toggle_button = toggleBtn
    gui_name_label = nameLabel
    gui_coord_label = coordLabel
    gui_avatar = avatar

    return { root = frame }
end

local function getPlayerList()
    local out = {}
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= lp and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
            table.insert(out, p)
        end
    end
    return out
end

local function equipFirstTool()
    local ch = lp.Character
    if ch and ch:FindFirstChildOfClass("Humanoid") then
        local h = ch:FindFirstChildOfClass("Humanoid")
        local bp = lp:FindFirstChild("Backpack")
        if bp then
            local tk = bp:FindFirstChildOfClass("Tool")
            if tk then
                h:EquipTool(tk)
            end
        end
    end
end

local function simClick()
    pcall(function()
        VIM:SendMouseButtonEvent(0, 0, 0, true, game, 0)
        task_wait(0.05)
        VIM:SendMouseButtonEvent(0, 0, 0, false, game, 0)
    end)
end

local function simE()
    pcall(function()
        VIM:SendKeyEvent(true, Enum.KeyCode.E, false, game)
        task_wait(0.05)
        VIM:SendKeyEvent(false, Enum.KeyCode.E, false, game)
    end)
end

buildGui()

task_spawn(function()
    while true do
        if RUNNING then
            -- Run a single cycle over available players
            local list = getPlayerList()
            if #list == 0 then
                if gui_name_label then gui_name_label.Text = "Target: None" end
                if gui_coord_label then gui_coord_label.Text = "Coords: N/A" end
                if gui_avatar then gui_avatar.Image = DEFAULT_AVATAR end
                task_wait(0.1)
            else
                for _, target in ipairs(list) do
                    if not RUNNING then break end
                    local startT = tick_f()
                    local thumb = safe(function()
                        return Players:GetUserThumbnailAsync(target.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
                    end)
                    if thumb and gui_avatar then gui_avatar.Image = thumb else if gui_avatar then gui_avatar.Image = DEFAULT_AVATAR end end

                    PLAYER_COUNTER = PLAYER_COUNTER + 1

                    if PLAYER_COUNTER % 4 == 0 and lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
                        lp.Character.HumanoidRootPart.CFrame = CFrame.new(SPECIAL_POS)
                        if gui_coord_label then gui_coord_label.Text = string.format("Coords: (%.1f, %.1f, %.1f)", SPECIAL_POS.X, SPECIAL_POS.Y, SPECIAL_POS.Z) end
                        equipFirstTool()
                        simE()
                        task_wait(2)
                    end

                    while RUNNING and tick_f() - startT < 1 do
                        if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                            local off = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -2)
                            lp.Character.HumanoidRootPart.CFrame = off
                            if gui_name_label then gui_name_label.Text = "Target: " .. target.Name end
                            local ppos = target.Character.HumanoidRootPart.Position
                            if gui_coord_label then gui_coord_label.Text = string.format("Coords: (%.1f, %.1f, %.1f)", ppos.X, ppos.Y, ppos.Z) end
                            equipFirstTool()
                            simClick()
                        end
                        task_wait(0.1)
                    end
                end
            end
        else
            task_wait(0.1)
        end
    end
end)

local function toggleLoop()
    RUNNING = not RUNNING
    if gui_toggle_button then
        gui_toggle_button.Text = RUNNING and S.disable or S.enable
    end
    if not RUNNING then
        if gui_name_label then gui_name_label.Text = "Target: None" end
        if gui_coord_label then gui_coord_label.Text = "Coords: N/A" end
        if gui_avatar then gui_avatar.Image = DEFAULT_AVATAR end
    end
end

if gui_toggle_button then
    gui_toggle_button.MouseButton1Click:Connect(toggleLoop)
else
    task_spawn(function()
        while not gui_toggle_button do task_wait(0.05) end
        gui_toggle_button.MouseButton1Click:Connect(toggleLoop)
    end)
end

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.Escape then
        if RUNNING then
            RUNNING = false
            if gui_toggle_button then gui_toggle_button.Text = S.enable end
            if gui_name_label then gui_name_label.Text = "Target: None" end
            if gui_coord_label then gui_coord_label.Text = "Coords: N/A" end
            if gui_avatar then gui_avatar.Image = DEFAULT_AVATAR end
        end
    end
end)

lp.CharacterAdded:Connect(function()
    task_wait(1)
    equipFirstTool()
end)

