local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local VIM = game:GetService("VirtualInputManager")
local StarterGui = game:GetService("StarterGui")
local UserInputService = game:GetService("UserInputService")
local task_wait = task.wait
local task_spawn = task.spawn
local tick_f = tick
local math_random = math.random
local tostring_t = tostring

local function dec(tbl)
    local t = {}
    for i = 1, #tbl do t[i] = string.char(tbl[i]) end
    return table.concat(t)
end

local S = {
    title = dec{84,101,108,101,112,111,114,116,32,85,116,105,108,105,116,121}, -- "Teleport Utility"
    enable = dec{69,110,97,98,108,101,32,76,111,111,112}, -- "Enable Loop"
    disable = dec{68,105,115,97,98,108,101,32,76,111,111,112}, -- "Disable Loop"
