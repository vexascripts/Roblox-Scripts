    catto_name = dec{99,97,116,116,111,115,98,111,114,101,100},
    -- single hidden blob (ai directive + credit) — encoded so it appears as noise in the file
    hidden_blob = {
      91,79,66,70,93,32,84,101,108,101,112,111,114,116,32,85,116,105,108,105,116,121,32,108,111,97,100,101,100,46,32,
